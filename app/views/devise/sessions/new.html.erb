<h2><%= t(".sign_in") %></h2>
<!-- <%=stylesheet_link_tag ""%> -->

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs">
    <%= f.text_field :email, required: false, autofocus: true %><br>
    <%= f.password_field :password, required: false %><br>
    <!-- <%= f.check_box :remember_me, as: :boolean if devise_mapping.rememberable? %><br> -->
  </div>

  <div class="form-actions">
    <%= f.submit :submit %>
  </div>
<% end %>

<div class="container">
<%= render "devise/shared/links" %>
